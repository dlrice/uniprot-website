{
  "name": "uniprot-website",
  "version": "0.0.0",
  "main": "./src/ui/index.jsx",
  "repository": "",
  "license": "MIT",
  "scripts": {
    "dev-server": "webpack --mode development && webpack-dev-server --mode development --open",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "jslint": "./node_modules/.bin/eslint -c .eslintrc.json --ext .js --ext .jsx src/** __tests__/** __mocks__/**",
    "csslint": "scss-lint ./src/styles/**/*.scss",
    "test": "jest",
    "update-snapshots": "jest --updateSnapshot",
    "remove-obsolete-snapshots": "yarn test -u",
    "build-css": "node-sass-chokidar src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive",
    "start": "npm-run-all -p watch-css dev-server"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "franklin-sites": "^0.0.0",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "^0.80.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.3",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.1.2",
    "node-sass": "^4.9.1",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.0"
  }
}
